{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "lib": ["dom", "ES2022"],
    "jsx": "preserve",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "allowJs": false,
    "strict": true,
    "noEmit": true,
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    }
  }
}
